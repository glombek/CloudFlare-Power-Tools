{
  "manifest_version": 2,

  "name": "CloudFlare Power Tools",
  "author": "Joe Glombek",
  "description": "Enhances the CloudFlare portal.",
  "version": "1",
  "homepage_url": "https://github.com/glombek/CloudFlare-Power-Tools",

  "permissions": [
    "https://www.cloudflare.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.cloudflare.com/*"],
      "css" : ["styles.css"],
      "js": ["jquery.js", "script.js"]
    }
  ]
}